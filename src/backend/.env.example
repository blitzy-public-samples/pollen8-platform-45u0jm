PORT=3000
NODE_ENV=development

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/pollen8
MONGODB_USER=example_user
MONGODB_PASSWORD=example_password

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=example_password

# AWS Configuration
AWS_ACCESS_KEY_ID=example_key_id
AWS_SECRET_ACCESS_KEY=example_secret
AWS_REGION=us-east-1
AWS_BUCKET_NAME=pollen8-assets

# SMS Configuration (Twilio)
TWILIO_ACCOUNT_SID=example_sid
TWILIO_AUTH_TOKEN=example_token
TWILIO_PHONE_NUMBER=+1234567890

# Geolocation Configuration (Google Maps)
GOOGLE_MAPS_API_KEY=example_api_key

# Authentication
JWT_SECRET=example_jwt_secret
JWT_EXPIRY=24h

# Logging
LOG_LEVEL=info
SENTRY_DSN=example_sentry_dsn

# Note: This file serves as a template for environment variables required by the backend service of the Pollen8 platform.
# It provides examples of all necessary configuration values while keeping actual sensitive data private.
# To use this file:
# 1. Copy this file and rename it to `.env`
# 2. Replace the example values with your actual configuration values
# 3. Never commit the actual `.env` file to version control
# 4. All team members should keep their `.env` files updated with the latest variables
# 5. Some values have example defaults, but should be changed in production