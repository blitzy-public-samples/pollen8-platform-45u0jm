{
  "name": "pollen8-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "lint": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "react": "latest",
    "react-dom": "latest",
    "d3": "latest",
    "tailwindcss": "latest",
    "socket.io-client": "latest"
  },
  "devDependencies": {
    "typescript": "latest",
    "vite": "latest",
    "jest": "latest",
    "@testing-library/react": "latest"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "./eslintrc.js"
    ],
    "ignorePatterns": [
      "dist",
      "node_modules"
    ]
  },
  "author": "Pollen8 Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git repository URL"
  }
}